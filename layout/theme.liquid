                  if (dropdownWrapper.classList.contains('active')) {
                      dropdownWrapper.classList.remove('active');
                      customWrappers[index].style.display = 'block';
                      dropdownIcons[index].style.transform = 'rotate(180deg)'; 
                  } else {
                      dropdownWrapper.classList.add('active');
                      customWrappers[index].style.display = 'none';
                      dropdownIcons[index].style.transform = 'rotate(359deg)';
                  }
              });
          });
      });
