          var customWrappers = document.querySelectorAll('.ais-facet-dropdown-container');
          var dropdownIcons = document.querySelectorAll('.ais-facet--header.ais-header .icon.icon-caret');
      
          customWrappers.forEach(function(wrapper) {
              wrapper.style.display = 'block'; 
