          cart_type: `{{ settings.cart_type }}`
        }
  
        window.variantStrings = {
          
          addToCart: `{{ 'products.product.add_to_cart' | t }}`,
          soldOut: `{{ 'products.product.sold_out' | t }}`,
          unavailable: `{{ 'products.product.unavailable' | t }}`,
          unavailable_with_option: `{{ 'products.product.value_unavailable' | t: option_value: '[value]' }}`,
        }
  
        window.accessibilityStrings = {
          imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
          shareSuccess: `{{ 'general.share.success_message' | t }}`,
          pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
          playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`,
        }
  
        window.shopUrl = `{{ request.origin }}`;

        {%- if request.page_type == 'product' -%}
        // We save the product ID in local storage to be eventually used for recently viewed section
        try {
          const items = JSON.parse(localStorage.getItem('recently-viewed-products') || '[]');
    
          // We check if the current product already exists, and if it does not, we add it at the start
          if (!items.includes({{ product.id | json }})) {
            items.unshift({{ product.id | json }});
          }
    
          localStorage.setItem('recently-viewed-products', JSON.stringify(items.slice(0, 20)));
        } catch (e) {
          // Safari in private mode does not allow setting item, we silently fail
        }
      {%- endif -%}
