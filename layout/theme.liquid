      $(document).ready(function () {
        function createCanvasAndDisplay(elementId) {
            const canvasElement = document.getElementById(elementId);
            console.log('canvasElement---', canvasElement);
            let productcanvas;
           
            if (window.innerWidth <= 768) {
                productcanvas = document.querySelector(".contents.mobileDeActive.canvas_mobile");
            } else {
                productcanvas = document.querySelector(".contents.mobileActive.ratingcard_canvas");
            }
    
            //let canvasWidth, canvasHeight;
            let canvasWidth = Math.min(productcanvas.offsetWidth, window.innerWidth, 1320);
            let canvasHeight = Math.min(productcanvas.offsetHeight, window.innerHeight);
            //console.log('check------',canvasWidth,canvasHeight);
          
            html2canvas(productcanvas, { allowTaint: true, useCORS: true, width: canvasWidth, height: canvasHeight, }).then(canvas => {
                canvasElement.appendChild(canvas);
                canvasElement.querySelector('canvas').setAttribute('id', 'canvasID');
                
    
                const canvasTP = document.getElementById('canvasID');
                console.log('canvasTP====', canvasTP);
    
                function convertCanvasToImage(canvas) {
                    return canvas.toDataURL('image/png');
                }
    
                let pnGImage = convertCanvasToImage(canvasTP);
                //console.log('pnGImage---', pnGImage);
    
                pnGImage = pnGImage.replace(/^data:image\/(png|jpg);base64,/, "");
                let productId = {{ product.id }};
                console.log('productId--', productId);
              
                    fetch('https://oyr2io95dd.execute-api.us-east-2.amazonaws.com/PROD/rating_card_image', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json',
                        },
                        body: JSON.stringify({
                            base_64: pnGImage,
                            product_id: productId
                        }),
                    })
                    .then(response => response.json())
                    .then(data => {
                      //console.log('API response:', data);
                      const altTagValue = data.altTag != null ? data.altTag : 'rating-card';
                      //console.log('Alt tag:', altTagValue);
                      if (altTagValue.includes("rating-card")) {
                          //console.log('Alt tag is right.');
                          
                        const img = new Image();
                        img.src = 'data:image/png;base64,' + pnGImage;
                        img.onload = function() {
                          const context = canvasTP.getContext('2d');
                          var wrh = img.width / img.height;
                          var newWidth = canvasTP.width - 20;
                          var newHeight = newWidth / wrh;
                          if (newHeight > canvasTP.height) {
                            newHeight = canvasTP.height - 20;
                            newWidth = newHeight * wrh;
                          }
                          var xOffset = newWidth < canvasTP.width ? ((canvasTP.width - newWidth) / 2) : 0;
                            var yOffset = newHeight < canvasTP.height ? ((canvasTP.height - newHeight) / 2) : 0;
  
                              context.drawImage(img, xOffset, yOffset, newWidth, newHeight);
                        };

                      } else {
                          //console.log('Alt tag  is not right.');
                      }
                    })
                  
                    .catch(error => {
                        console.error('API error:', error);
                    });
            })
            .catch(error => {
                console.error('html2canvas error:', error);
            });
        }
          createCanvasAndDisplay('rating_html');
          //createCanvasAndDisplay('rating_htmls');
      });
