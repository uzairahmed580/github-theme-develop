    background-color: #fff;
  }
  .kyx-advantage-banner img {
    width: 100%;
    height: auto;
  }
  .kyx-advantage-heading {
    background-color: #f2eeeb;
    padding-bottom: 5rem;
  }
  .md-hidden {
    display: none;
  }
  .md-visible {
    display: block;
  }
