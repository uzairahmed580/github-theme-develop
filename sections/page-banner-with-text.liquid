{% schema %}
  {
    "name": "Page Banner with text",
    "settings": [
      {
        "type": "image_picker",
        "id": "image",
        "label": "Image"
      }, {
        "type": "image_picker",
        "id": "sm_image",
        "label": "Mobile Image"
      }, {
        "type": "text",
        "id": "title",
        "label": "Heading"
      }, {
        "type": "textarea",
        "id": "sub_title",
        "label": "Sub Title"
      }
    ],
    "presets": [
      {
        "name": "Page Banner with text"
      }
    ]
  }
{% endschema %}