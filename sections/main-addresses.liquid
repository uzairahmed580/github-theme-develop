<script>
function isCharacterKey(event) {
  var keyCode = event.keyCode || event.which;
  var charKey = String.fromCharCode(keyCode);
  
  // Regular expression to match alphabetic characters
  var regex = /^[a-zA-Z]+$/;
  
  if (regex.test(charKey)) {
    return true; // Allow character key
  } else {
    return false; // Block non-character key
  }
}
