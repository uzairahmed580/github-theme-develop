<div class="back_home cartPopUp" style="display:none;" id="popupwish">
    <div class="page-width" id="addtocartmsg" style="text-align:center;">
      Product added to cart it will be removed after 10 mins<br/>
      {{ product.title }}-{{ product.price | money_with_currency }}
    </div>
</div>
<div class="back_home mobileActive page-width">
  <p id="product_id" style="display:none">{{ product.id }}</p>
    <div style="display: flex; align-items:center">
      <a class="tags" href="{{ routes.root_url }}">Home</a>  
      {% if product.metafields.custom.Brand %}
        <img src="{{ 'side_icon.png' | asset_url }}" style="height: 1rem; width: 1rem" alt="side" />
        <a class="tags" href="https://www.kyxsneakers.com/search?refinementList%5Bmeta.custom.Brand%5D%5B0%5D={{ product.metafields.custom.Brand }}">{{ product.metafields.custom.Brand }}</a>
      {% endif %}
      <img src="{{ 'side_icon.png' | asset_url }}" style="height: 1rem; width: 1rem" alt="side" />
      <div class="tags">{{ product.title | escape }}</div>
    </div>
</div>  
<div class="product_container">
  <div id="productView" class="page-width">
    {% comment %} <div class="sticky-btn-mobile">
      <div class="pinkButton mobileActiveButton">                             
        {% for variant in product.variants %}
          {% if variant.inventory_quantity < 0 %}
            <button class="cartButtonOut1"  style="display:block;color: red;width:100%; " disabled>SOLD</button>
            {% else %}					
              <div id="heart">
                  <button class="my-icon swym-button swym-add-to-wishlist swym-iconbtnlink swym-heart swym-added swym-loaded" data-with-epi="true" data-swaction="addToWishlist" data-product-id="{{ product.id }}" data-variant-id="{{ product.variants[0].id }}" data-product-url="{{shop.url}}{{product.url}}">
                                <i class="fa fa-heart-o"></i></button>                
              </div>
              <!-- 6/05/23 rupali --> 

              {% assign x = 0 %}
              {% if cart.item_count == 0 %}
              <button class="ad_to_cart hideme"  var_id="{{ product.variants.first.id }}" style="color: #e5337c">ADD TO CART</button>
              <button class="cartButtonOut1"  style="display:none;color: red;width:100%; " disabled>SOLD</button>
              <button class="cartButtonOut2"  style="display:none;color: red;width:100%; " disabled>ADDED TO CART</button>
              {% else %}
              {% for item in cart.items %}
                {% if product.id == item.product_id %}
                {% assign x = 1 %}
                <button class="cartButtonOut1" style="width:100%; color:red;"  disabled>ADDED TO CART</button>
                {% endif %}
              {% endfor -%}
              {% if x == 0 %}
                <button class="ad_to_cart hideme"  var_id="{{ product.variants.first.id }}" style="color: #e5337c">ADD TO CART</button>
                <button class="cartButtonOut3"  style="display:none;color: red;width:100%; " disabled>ADDED TO CART</button>
              {% endif %}
              {% endif %}						
          {% endif %}
        {% endfor %}             

        <form method="post" action="/cart/add" style="display:none;" id="buy_now">
          <input name="id" value="{{ product.variants.first.id }}" type="hidden">
          <input type="hidden" name="return_to" value="checkout">                  
        </form>
        {% if x == 0 %}   
          <button type="button" id="buynowbutton" class="hideme buynowtimer" var_id="{{ product.variants.first.id }}" style="color: white; background: #e5337c" >BUY NOW</button>
        {% endif %}            
      </div>
    </div> {% endcomment %}

